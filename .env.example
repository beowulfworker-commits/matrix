# .env.example
# Домашнее имя Matrix-сервера (должно совпадать с server_name в homeserver.yaml)
# Пример: matrix.example.com
MATRIX_SERVER_NAME=matrix.example.com

# Email администратора для Let's Encrypt
MATRIX_ADMIN_EMAIL=admin@example.com

# Отправлять ли анонимную статистику разработчикам Matrix (yes|no)
SYNAPSE_REPORT_STATS=no

# Параметры базы данных PostgreSQL
POSTGRES_DB=synapse
POSTGRES_USER=synapse
POSTGRES_PASSWORD=CHANGE_ME_PLEASE
